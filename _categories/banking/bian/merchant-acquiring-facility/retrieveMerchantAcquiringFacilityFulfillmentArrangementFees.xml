<unit xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="main" xsi:noNamespaceSchemaLocation="http://atm.ibm.com/app/unit.xsd">
  <requirements>
  </requirements>
  <configs>
  </configs>
  <sequence name="main">
    <get url="${protocol}${domain}${basePath}${endpoint}" params="[:]" var="payload" mode="json" gen="e2f">
      <header name="Accept" value="application/json" gen="e2f"/>
    </get>
    <assert-exists expression="payload"  mode="all" level="error" stoponfail="false" gen="jag"/>
    <assert-exists expression="payload.merchantAcquiringFeeReference"  mode="all" level="error" stoponfail="false" gen="jag"/>
    <assert-exists expression="payload.productInstanceReference"  mode="all" level="error" stoponfail="false" gen="jag"/>
    <assert-exists expression="payload.merchantAcquiringFeeConfiguration"  mode="all" level="error" stoponfail="false" gen="jag"/>
    <assert-exists expression="payload.merchantAcquiringFeeType"  mode="all" level="error" stoponfail="false" gen="jag"/>
    <assert-exists expression="payload.merchantAcquiringFeeCharge"  mode="all" level="error" stoponfail="false" gen="jag"/>
    <assert-exists expression="payload.merchantAcquiringDateTime"  mode="all" level="error" stoponfail="false" gen="jag"/>
    <assert-exists expression="payload.merchantAcquiringFeeProjectionsCommitments"  mode="all" level="error" stoponfail="false" gen="jag"/>
  </sequence>
</unit>
