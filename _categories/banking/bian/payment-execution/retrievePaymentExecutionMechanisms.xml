<unit xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="main" xsi:noNamespaceSchemaLocation="http://atm.ibm.com/app/unit.xsd">
  <requirements>
  </requirements>
  <configs>
  </configs>
  <sequence name="main">
    <get url="${protocol}${domain}${basePath}${endpoint}" params="[:]" var="payload" mode="json" gen="e2f">
      <header name="Accept" value="application/json" gen="e2f"/>
    </get>
    <assert-exists expression="payload"  mode="all" level="error" stoponfail="false" gen="jag"/>
    <assert-exists expression="payload.correspondentServiceAccessSchedule"  mode="all" level="error" stoponfail="false" gen="jag"/>
    <assert-exists expression="payload.paymentExecutionTransaction"  mode="all" level="error" stoponfail="false" gen="jag"/>
    <assert-exists expression="payload.correspondentServiceSessionReference"  mode="all" level="error" stoponfail="false" gen="jag"/>
    <assert-exists expression="payload.paymentTransactionMechanismReference"  mode="all" level="error" stoponfail="false" gen="jag"/>
    <assert-exists expression="payload.paymentTransactionRequestReference"  mode="all" level="error" stoponfail="false" gen="jag"/>
    <assert-exists expression="payload.achAccessServiceSessionReference"  mode="all" level="error" stoponfail="false" gen="jag"/>
    <assert-exists expression="payload.achAccessSchedule"  mode="all" level="error" stoponfail="false" gen="jag"/>
  </sequence>
</unit>
